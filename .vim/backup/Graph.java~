/*
 * represents a collection of connected vertices
 */
public class Graph {
   ArrayList<Vertex> members;

   public Graph(){
       members = new ArrayList<Vertex>();
   }

   public void addNeighbor(Vertex v, Vertex n){

   }

   public void addVertex(Vertex toAdd){
	//adds vertex
	this.members.add(toAdd);
   }
}

public static void main(String[] args){
    Graph closeFriends = new Graph();
    Graph family = new Graph();

    closeFriends.addVertex(new Vertex(1));
}

/*
 * represents a vertex
 */
public class Vertex {
    int data;
    ArrayList<Vertex> neighbors;
    
    public void addNeighbor(){

    }

    public Vertex(int data){
	this.data = data;
	neighbors= new ArrayList<Vertex>();
    }
}


<!DOCTYPE html>
<head>
    <title>Number Guessing Game</title>
    <script>
	var ans = Math.floor((Math.random()*100)+1);
	var input = prompt("Guess a number!");
	count = 0;
	while (input != ans) {
	    input = prompt("Guess a number again!");
	    count++;
	}
	console.log(count);
    </script>
</head>
<body>
</body>
